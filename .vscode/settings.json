{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "node_modules": true
    },
    "typescript.referencesCodeLens.enabled": true,
    "typescript.implementationsCodeLens.enabled": true,
    "editor.tabCompletion": "on",
    "editor.minimap.enabled": true,
    "editor.mouseWheelZoom": true,
    "editor.formatOnSave": true
}